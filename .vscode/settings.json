{
  "editor.rulers": [150],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2f537c",
    "activityBar.background": "#2f4e7c",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#422c74",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#4a2f7c",
    "statusBar.background": "#312157",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#3d2f7c",
    "statusBarItem.remoteBackground": "#2d2157",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#292157",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#2b215799",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
